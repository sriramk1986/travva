document.getElementById('alerts').innerHTML = "<%= j render 'shared/flashes' %>"
$('<%= "#dropdownTripButton#{@item_id}" %>').dropdown('toggle');


<% if @trip_item.persisted? %>
var newDayItem = document.querySelector('.cards-small-item', '.draggable-items')
var content = "<%= j render 'items/trip_card', trip_item: @trip_item %>"
if (newDayItem) {
  newDayItem.insertAdjacentHTML('beforeend', content);
}
document.querySelector("#map").dataset.markers = '<%= @markers.to_json.html_safe %>';
buildMap();
<% end %>
